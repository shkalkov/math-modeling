{
   "extends": ["airbnb-base", "prettier"],
   "plugins": ["prettier"],
   "rules": {
      "func-names": 0,
      "prettier/prettier": ["error"]
   },
   "env": {
      "browser": true,
      "es6": true
   },
   "globals": {
      "document": true,
      "browser": true,
      "window": true
   }
}
