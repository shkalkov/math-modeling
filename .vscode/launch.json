{
   "version": "0.2.0",
   "configurations": [
      {
         "type": "chrome",
         "request": "attach",
         "name": "Attach to Chrome",
         "port": 5500,
         "webRoot": "${workspaceFolder}"
      },
      {
         "type": "chrome",
         "request": "launch",
         "name": "Launch Chrome",
         "url": "http://localhost:5500",
         "webRoot": "${workspaceFolder}"
      },
      {
         "type": "node",
         "request": "launch",
         "name": "Запустить программу",
         "program": "${workspaceFolder}/lab-works/l2/app/js/index.js"
      },
      {
         "type": "node",
         "request": "launch",
         "name": "Stat values",
         "program": "${workspaceFolder}/lab-works/l2/app/js/statValues.js"
      }
   ]
}
